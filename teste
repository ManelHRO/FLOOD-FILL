#include <bits/stdc++.h>
using namespace std;
int main(){
	
	int n,soma,i,j;
	cin>>n;
	int mt[n][n];
	
	for(int i=0;i<n;i++){
		for(int j=0;j<n;j++){
			cin>>mt[i][j];
		}
	}
	for(int k=0;k<=n/2;k++){
		soma=0;
		for(j=k;j<n-k;j++){
			soma+=mt[k][j];
		}
		for(i=k+1;i+1<n-k;i++){
			soma+=mt[i][j-1];
		}
		for(j=j-1;j>=k;j--){
			soma+=mt[i][j];
		}
		for(i=i-1,j+=1;i>k;i--){
			soma+=mt[i][j];
		}
		cout<<soma<<endl;
	}
}
